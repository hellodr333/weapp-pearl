<!--mine.wxml-->
<view>
  <swiper indicator-dots="true"
    autoplay="true" interval="{{interval}}" duration="{{duration}}" circular="true">
    <block wx:for="{{imgUrls}}" wx:key="key">
      <swiper-item >
        <image src="{{item}}" class="slide-image img"  mode="aspectFill"/>
      </swiper-item>
    </block>
  </swiper>
</view>